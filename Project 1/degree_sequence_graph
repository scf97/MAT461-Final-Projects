{
 "cells": [
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {
    "collapsed": false
   },
   "outputs": [
   ],
   "source": [
    "'''\n",
    "Returns a graph for the corresponding degree sequence\n",
    "\n",
    "Parameters:\n",
    "\n",
    "seq: list degree sequence\n",
    "show_graph: (Optional) displays graph using sage .show() function, default: True\n",
    "print_steps: (Optional) prints each step of Havel-Hakimi algorithm, default: False\n",
    "\n",
    "Returns:\n",
    "\n",
    "Dictionary for use in creating a sage graph\n",
    "'''\n",
    "def degree_sequence_graph(seq:list, show_graph:bool=True, print_steps:bool=False) -> dict:\n",
    "\n",
    "    # Ensure sequence is valid\n",
    "    if sum(seq) % 2 == 1:\n",
    "        return {}\n",
    "\n",
    "    # sort in descending order\n",
    "    seq.sort(reverse=True)\n",
    "\n",
    "    # Convert to list form\n",
    "    seq_list = [ [seq[i], i] for i in range(len(seq)) ]\n",
    "\n",
    "\n",
    "    # create output graph dictionary\n",
    "    graph_dict = { i: [] for i in range(len(seq_list)) }\n",
    "\n",
    "\n",
    "\n",
    "    if print_steps:\n",
    "        print(seq_list)\n",
    "\n",
    "    while True:\n",
    "\n",
    "        # check minimum and maximum degree\n",
    "        min = 1\n",
    "        max = 0\n",
    "        for i in range(len(seq_list)):\n",
    "            if seq_list[i][0] < min:\n",
    "                min = seq_list[i][0]\n",
    "            if seq_list[i][0] > max:\n",
    "                max = seq_list[i][0]\n",
    "\n",
    "        if min < 0:\n",
    "            return {}\n",
    "        if max == 0:\n",
    "            break\n",
    "        if max > (len(seq_list)-1):\n",
    "            return {}\n",
    "\n",
    "\n",
    "        # add neighbors of first element in list to graph dictionary\n",
    "        d = seq_list[0][0]\n",
    "        v = seq_list[0][1]\n",
    "        graph_dict[v] = [seq_list[u][1] for u in range(1,d+1)]\n",
    "\n",
    "\n",
    "        # remove first element and reduce neighbor degrees\n",
    "        seq_list.pop(0)\n",
    "        for i in range(d):\n",
    "            seq_list[i][0] -= 1\n",
    "\n",
    "        # sort list using selection sort\n",
    "        for i in range(len(seq_list)):\n",
    "            max_index = i\n",
    "            for j in range(i+1, len(seq_list)):\n",
    "                if seq_list[j][0] > seq_list[max_index][0]:\n",
    "                    max_index = j\n",
    "\n",
    "            swap = seq_list[max_index]\n",
    "            seq_list[max_index] = seq_list[i]\n",
    "            seq_list[i] = swap\n",
    "\n",
    "\n",
    "        # prints each step of algorithm if print_steps==True\n",
    "        if print_steps:\n",
    "            print(seq_list)\n",
    "\n",
    "\n",
    "    # shows graph if show_graph==True\n",
    "    if (show_graph):\n",
    "        Graph(graph_dict).show()\n",
    "\n",
    "\n",
    "    # returns dictionary for sage Graph constructor\n",
    "    return graph_dict"
   ]
  },
